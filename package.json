    "build:prod": "webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
    "build:aot:prod": "webpack --config config/webpack/webpack.prod.js  --progress --profile --bail",
    "server:prod": "webpack-dev-server --config config/webpack/webpack.prod.js --open --progress --profile --watch",
